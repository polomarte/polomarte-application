<table>
  <tr>
    <th>Name</th>
    <th>Status</th>
  </tr>
  <% @projects.each do |project| %>
  <tr>
  <td> <%= project.name %>    </td>
  <td> <%= project.status %>  </td>
  <td> <%= link_to "Edit", edit_project_path(project) %> </td>
  <td> <%= link_to "Destroy",{action: :destroy, id: project.id}, method: :delete, data: { confirm: "Are you sure?" } %> </td>
  </tr>
  <% end %>
</table>

<br>
<%= link_to "New Project", new_project_path %>
